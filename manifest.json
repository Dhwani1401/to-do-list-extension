{
  "name": "To-Do List",
  "description": "This function organizes tasks in the form of a To-Do List",
  "version": "1.0",
  "manifest_version":2,
"browser_action":{
  "default_icon":"icon.png",
  "default_popup":"popup.html"},
  "permissions":[
    "storage"
],
    "content_scripts":[
      {
        "matches":["<all_urls>"],
        "css":["popup.css"],
        "js": ["popup.js"]
      }
    ]
}
